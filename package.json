    "prettier": "2.2.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "postcss": "8.2.1",
    "react-test-renderer": "17.0.1",
    "slate": "0.60.2",
    "slate-history": "0.60.2",
    "slate-react": "0.60.2",
    "start-server-and-test": "1.12.0",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3"
  }
